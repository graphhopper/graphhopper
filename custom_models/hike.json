{
  "comment1": "use this custom model with the foot vehicle",
  "comment2": "do not forget to add: graph.encoded_values: hike_rating,average_slope,max_slope",
  "priority": [
    { "if": "hike_rating < 4 && foot_access == false", "multiply_by": "0"},
    { "if": "foot_network == INTERNATIONAL || foot_network == NATIONAL", "multiply_by": "1.7"},
    { "else_if": "foot_network == REGIONAL || foot_network == LOCAL", "multiply_by": "1.4"}
  ],
  "speed": [
    { "if": "hike_rating < 4", "limit_to": "foot_average_speed" },
    { "else":   "", "limit_to": "3" },
    { "if":      "average_slope >= 20", "limit_to": "1"},
    { "else_if": "average_slope >= 10", "limit_to": "2"},
    { "else_if": "average_slope >=  4", "multiply_by": "0.85"},
    { "else_if": "average_slope <= -4", "multiply_by": "1.05"}
  ]
}