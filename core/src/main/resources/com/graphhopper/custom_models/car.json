// to use this custom model you need to set the following option in the config.yml
// graph.encoded_values: car_access|block_private=false, car_average_speed, road_access, max_speed, ferry_speed
// profiles:
//    - name: car
//      turn_costs:
//        vehicle_types: [motorcar, motor_vehicle]
//      custom_model_files: [car.json, car_avoid_private_etc.json ]

{
  "distance_influence": 90,
  "priority": [
    { "if": "!car_access", "multiply_by": "0" }
  ],
  "speed": [
    { "if": "road_environment == FERRY", "limit_to": "ferry_speed" },
    { "else": "", "limit_to": "car_average_speed" },
    { "if": "true", "limit_to": "max_speed * 0.9" }
  ]
}
